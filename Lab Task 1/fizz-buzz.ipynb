    "def fizz_buzz(start,end):\n",
    "    for i in range(start, end + 1):\n",
    "     if i % 3 == 0 and i % 5 == 0:\n",
    "        print(\"FizzBuzz\")\n",
    "     elif i % 3 == 0:\n",
    "        print(\"Fizz\")\n",
    "     elif i % 5 == 0:\n",
    "        print(\"Buzz\")\n",
    "     else:\n",
    "        print(i)\n",
    "\n",
    "fizz_buzz(1,5)"
   ]
